#include <stdio.h>

int main()
{
    int num;
    scanf("%d",&num);
    switch (num){
        case 1:
            printf("Topper");
            break;
        case 2:
            printf("Excellent");
            break;
        case 3:
            printf("Very Good");
            break;
        case 4:
            printf("Good");
            break;
        case 5:
            printf("Super");
            break;
        case 6:
            printf("Average");
            break;
        case 7:
            printf("Okay");
            break;
        default:
            printf("Start a Thing");
    }
    return 0;
}
